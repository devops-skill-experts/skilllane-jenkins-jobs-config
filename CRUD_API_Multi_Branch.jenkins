#!/usr/bin/env groovy

multibranchPipelineJob('CRUD-API')
{
    displayName('CRUD_API_Multi_Branch')
    branchSources
    {
        git
        {
            id('skilllane-crud-example')
            remote('https://github.com/devops-skill-experts/skilllane-crud-example.git')
            credentialsId('')
            includes('develop test master main')
        }
    }

    triggers
    {
        cron('H/1 * * * *')
    }
}
